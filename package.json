  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@apollo/client": "^3.7.1",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@tinymce/tinymce-react": "^4.2.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "antd": "^4.24.1",
    "axios": "^1.1.3",
    "dayjs": "^1.11.6",
    "graphql": "^16.6.0",
    "humanize-duration": "^3.27.3",
    "lodash": "^4.17.21",
    "numeral": "^2.0.6",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.3",
    "react-scripts": "5.0.1",
    "react-use": "^17.4.0",
    "store": "^2.0.12",
    "typescript": "^4.8.4",
    "use-query-params": "^2.1.2",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "PORT=4200 REACT_APP_VERSION=$npm_package_version react-scripts start",
    "build": "PORT=4200 REACT_APP_VERSION=$npm_package_version react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "format": "prettier --write --ignore-unknown '**/*.{js,jsx,ts,tsx,json,md,css,graphql,gql}'",
    "codegen": "graphql-codegen --config codegen.ts && yarn format"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.13.11",
    "@graphql-codegen/client-preset": "^1.1.3",
    "@graphql-codegen/typescript": "^2.8.1",
    "@graphql-codegen/typescript-operations": "^2.5.6",
    "@graphql-codegen/typescript-react-apollo": "^3.3.6",
    "@types/humanize-duration": "^3.27.1",
    "@types/lodash": "^4.14.188",
    "@types/mime-types": "^2.1.1",
    "@types/numeral": "^2.0.2",
    "@types/store": "^2.0.2",
    "@types/uuid": "^8.3.4",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false,
    "arrowParens": "always",
    "printWidth": 80
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,md,css,graphql,gql}": "yarn format"
  }
}
